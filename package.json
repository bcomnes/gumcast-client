{
  "name": "gumcast-client-2",
  "description": "WIP - nothing to see here",
  "version": "0.0.0",
  "private": true,
  "author": "Bret Comnes <bcomnes@gmail.com> (https://bret.io)",
  "bugs": {
    "url": "https://github.com/bcomnes/gumcast-client-2/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "auto-changelog": "^1.16.2",
    "browser-sync": "^2.26.7",
    "cpx2": "^2.0.0",
    "dependency-check": "^4.1.0",
    "gh-release": "^3.5.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard": "^13.1.0",
    "tap": "^14.10.2"
  },
  "homepage": "https://github.com/bcomnes/gumcast-client-2",
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcomnes/gumcast-client-2.git"
  },
  "scripts": {
    "prepublishOnly": "git push --follow-tags && gh-release",
    "test": "run-s test:*",
    "test:deps": "dependency-check . --no-dev --no-peer",
    "test:standard": "standard",
    "test-skip:tap": "tap",
    "start": "run-s watch",
    "version": "auto-changelog -p --template keepachangelog auto-changelog --breaking-pattern 'BREAKING CHANGE:' && git add CHANGELOG.md",
    "build": "npm run clean && run-p build:*",
    "build:static": "cpx 'src/**/*' public",
    "watch": "npm run clean && run-p watch:* build:static",
    "watch:static": "npm run build:static -- --watch",
    "watch:serve": "browser-sync start --server 'public' --files 'public'",
    "clean": "rimraf public && mkdirp public"
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  }
}
