{
  "name": "gumcast-client",
  "description": "Web app frontend for Gumcast.com",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "bugs": {
    "url": "https://github.com/gumcast/gumcast-client/issues"
  },
  "devDependencies": {
    "@bret/siteup": "^0.0.11",
    "auto-changelog": "^1.16.2",
    "browser-sync": "^2.26.7",
    "gh-release": "^3.5.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard": "^14.0.0",
    "tap": "^14.10.2"
  },
  "homepage": "https://github.com/gumcast/gumcast-client",
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gumcast/gumcast-client.git"
  },
  "scripts": {
    "prepublishOnly": "git push --follow-tags && gh-release",
    "test": "run-s test:*",
    "test:standard": "standard",
    "test-skip:tap": "tap",
    "start": "run-s watch",
    "version": "auto-changelog -p --template keepachangelog auto-changelog --breaking-pattern 'BREAKING CHANGE:' && git add CHANGELOG.md",
    "build": "npm run clean && run-p build:*",
    "build:siteup": "siteup",
    "watch": "npm run clean && run-p watch:*",
    "watch:siteup": "npm run build:siteup -- --watch",
    "watch:serve": "browser-sync start --server 'public' --files 'public'",
    "clean": "rimraf public && mkdirp public"
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  },
  "dependencies": {
    "@datadog/browser-rum": "^3.2.0",
    "fetch-undici": "^1.0.3",
    "local-storage-proxy": "^3.0.3",
    "mine.css": "^4.6.1",
    "uhtml-isomorphic": "^0.0.3"
  }
}
